{
    "name": "innmind/ark",
    "type": "library",
    "description": "Servers' array management",
    "keywords": ["server", "management"],
    "homepage": "http://github.com/Innmind/Ark",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Ark/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/url": "^2.0",
        "innmind/immutable": "^2.8",
        "ovh/ovh": "^2.0",
        "innmind/server-control": "^2.3",
        "innmind/operating-system": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Ark\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Ark\\": "tests/",
            "Fixture\\Innmind\\Ark\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "giorgiosironi/eris": "^0.9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
